<%- include("partials/header"); -%>
<h1>Home</h1>
<p><%= startingContent %></p>
<button ><a href="/compose"> New </a></button>
<% newContents.forEach(function(newContent){ %>
<div class="postTitle">
  <h1><%=newContent.title%></h1>
  <div class="postContent">
    <p><%=newContent.content.substring(0,100) + "...."%>
      <a href="/posts/<%= newContent.title %>">Read More</a>
    </p>
  </div>
</div>



<% }) %>




